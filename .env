# Environment Variables untuk Bot WhatsApp
# Hapus tanda # untuk mengaktifkan variable yang dibutuhkan

# Bot Configuration
BOT_NAME=wa-request-bot
BOT_VERSION=1.0.0

# WhatsApp Web Configuration
# SESSION_ID=your-session-id-here
# WEBHOOK_URL=your-webhook-url-here

# Server Configuration
PORT=3000
NODE_ENV=production

# Bot Settings
# PREFIX=!
# AUTO_REPLY=true
# WELCOME_MESSAGE=true

# Keep Alive Settings
KEEP_ALIVE=true
PING_INTERVAL=300000

# Logging
LOG_LEVEL=info
DEBUG=false
